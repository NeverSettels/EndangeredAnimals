

<h1>All Animals:</h1>

<ol>
@foreach (Animal animal in Model)
{
    <ul>
      <li>Name: @animal.taxonname</li>
      <li>Species: @animal.scientific_name</li>
      <li><img src="@animal.imgURL" alt="Sample Image" width="300px" /></li>
      
    </ul>
    @* <a href="/Animals/Details/@(animal.AnimalId)"><p>View Details</p></a> *@
    <br>
}
</ol>